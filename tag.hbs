{{!< default }}
<aside class="sidebar hide-on-small-only">
		{{> sidebar }}
</aside>
<main class="main">
    <div class="hide-on-med-and-up">
				{{> navbar }}
    </div>
    <section>
        <div class="row">
            <div class="col s12">
                <div class="card blue-grey darken-1">
                {{#tag}}
                    <div class="card-content white-text center">
                        <span class="card-title">Articles Tagged {{ name }} </span>
                    </div>
                {{/tag}}
                </div>
            </div>
        </div>
				{{> loop/default }}
    </section>
</main>
{{> footer }}